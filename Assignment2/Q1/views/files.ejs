<!DOCTYPE html>
<html>
<head>
  <title>Uploaded Files</title>
</head>
<body>
  <h1>Uploaded Files</h1>
  <ul>
    <% users.forEach(user => { %>
      <% user.files.forEach(file => { %>
        <li>
          <a href="/download/<%= file %>"><%= file %></a>
        </li>
      <% }) %>
    <% }) %>
  </ul>
</body>
</html>
